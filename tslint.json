{
  "rules": {
    "no-console": [true, "debug", "info", "log", "time", "timeEnd", "trace"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "extends": ["tslint:latest", "tslint-config-prettier"]
}
